{
    "name": "utilisateur/dwwm_php_ci",
    "autoload": {
        "psr-4": {
            "Src\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Este98",
            "email": "este98@hotmail.fr"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^12.4",
        "phpstan/phpstan": "^2.1",
        "friendsofphp/php-cs-fixer": "^3.89"
    },
    "scripts": {
        "test": "phpunit",
        "stan-check": "phpstan analyse --level=5 src",
        "cs-check": "php-cs-fixer fix --dry-run --using-cache=no --verbose --allow-risky=yes"
    }
}
